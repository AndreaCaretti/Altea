{
	"_version": "1.12.0",
	"sap.app": {
		"id": "cloudcoldchain.controlpoints",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"CAP_Services": {
				"uri": "/srv_api/services/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"title": "Manage ControlPoints",
		"description": "ControlPoints Maintenance",
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "ControlPoints",
					"action": "manage",
					"title": "Manage ControlPoints",
					"description": "ControlPoints Maintenance",
					"icon": "sap-icon://add"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"fullWidth": false,
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.fiori": {
		"registrationIds": [],
		"archeType": "transactional"
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.78.0",
			"libs": {
				"sap.fe.templates": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "cloudcoldchain.controlpoints.i18n.i18n"
				}
			},
			"": {
				"dataSource": "CAP_Services",
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true,
					"groupProperties": {
						"default": {
							"submit": "Auto"
						}
					}
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "ControlPointsList",
					"target": "ControlPointsList"
				},
				{
					"pattern": "ControlPoints({key}):?query:",
					"name": "ControlPointDetails",
					"target": "ControlPointDetails"
				}
			],
			"targets": {
				"ControlPointsList": {
					"type": "Component",
					"id": "ControlPointsList",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"entitySet": "ControlPoints",
							"navigation": {
								"ControlPoints": {
									"detail": {
										"route": "ControlPointDetails"
									}
								}
							}
						}
					}
				},
				"ControlPointDetails": {
					"type": "Component",
					"id": "ControlPointDetails",
					"name": "sap.fe.templates.ObjectPage",
					"options": {
						"settings": {
							"entitySet": "ControlPoints"
						}
					}
				}
			}
		}
	}
}
